import random

def get_random_fact():
    facts = [
        "The first paper money was created in China during the Tang Dynasty.",
        "A man once sold a $1 million dollar bill for just $2.",
        "In 2008, Iceland became the first country to elect an openly gay head of state.",
        "There are more $100 bills in circulation than any other denomination."
    ]
    return random.choice(facts)

